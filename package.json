{
  "name": "taller_api",
  "version": "0.0.1",
  "description": "Repositorio para el taller de APIs con Node.js",
  "main": "app.js",
  "scripts": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xoscar/taller_api.git"
  },
  "keywords": [
    "taller",
    "itl",
    "api",
    "node"
  ],
  "author": "Oscar Reyes <oscar-rreyes1@hotmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xoscar/taller_api/issues"

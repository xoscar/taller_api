  "homepage": "https://github.com/xoscar/taller_api#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "morgan": "^1.7.0",
    "request": "^2.75.0"
